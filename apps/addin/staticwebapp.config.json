{
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".jsx": "text/javascript",
    ".ts": "text/javascript",
    ".tsx": "text/javascript",
    ".xml": "application/xml"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type",
    "X-Content-Type-Options": "nosniff"
  },
  "routes": [
    {
      "route": "/manifest.xml",
      "headers": {
        "Content-Type": "application/xml; charset=utf-8",
        "Cache-Control": "public, max-age=300"
      }
    },
    {
      "route": "/*.xml",
      "headers": {
        "Content-Type": "application/xml; charset=utf-8"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/icons/*",
      "/*.css",
      "/*.js",
      "/*.mjs",
      "/*.png",
      "/*.gif",
      "/*.ico",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.woff",
      "/*.woff2",
      "/*.json",
      "/*.xml"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
