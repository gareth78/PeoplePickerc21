{
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".jsx": "text/javascript",
    ".ts": "text/javascript",
    ".tsx": "text/javascript",
    ".xml": "application/xml"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/icons/*",
      "/*.css",
      "/*.js",
      "/*.mjs",
      "/*.png",
      "/*.gif",
      "/*.ico",
      "/*.jpg",
      "/*.jpeg",
      "/*.svg",
      "/*.woff",
      "/*.woff2",
      "/*.json",
      "/*.xml"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
